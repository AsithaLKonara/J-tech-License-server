{
  "chip_id": "attiny85",
  "chip_variant": null,
  "chip_name": "ATtiny85",
  "manufacturer": "Microchip",
  "architecture": "AVR",
  "flash_size_bytes": 8192,
  "ram_size_bytes": 512,
  "cpu_frequency_mhz": 8,
  "supported_formats": ["hex"],
  "toolchain": {
    "compiler": "avr-gcc",
    "linker": "avr-ld",
    "objcopy": "avr-objcopy",
    "flasher": "avrdude",
    "version_required": "6.3"
  },
  "flash_options": {
    "default_baud_rate": 19200,
    "supported_baud_rates": [9600, 19200, 38400],
    "programmer": "usbtiny",
    "flash_size": "8KB"
  },
  "build_options": {
    "defines": ["__AVR_ATtiny85__"],
    "include_paths": [],
    "linker_script_required": false,
    "fuses_required": true,
    "bootloader_required": false
  },
  "verification": {
    "supports_hash_verification": true,
    "verification_method": "avrdude_verify",
    "hash_algorithm": "sha256"
  },
  "device_detection": {
    "method": "avrdude_read_signature",
    "timeout_ms": 5000,
    "baud_rate": 19200
  },
  "capabilities": ["flash", "verify"]
}

